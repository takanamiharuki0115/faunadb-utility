{
  "name": "faunadb-utility",
  "version": "0.2.0",
  "description": "Set of useful helper functions to use with FaunaDB",
  "main": "dist/index",
  "typings": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx awesome-readme && npm run lint:fix && tsc --project tsconfig.json",
    "lint:fix": "prettier --write src/**/*.{js,ts} && tslint --fix --config tslint.json --project tsconfig.json",
    "lint": "tslint --config tslint.json --project tsconfig.json",
    "watch": "tsc -w"
  },
  "files": [
    "/dist/**/*.{js,ts,map}",
    "/src/**/*.{js,ts}"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marc-aurele-besner/faunadb-utility.git"
  },
  "keywords": [
    "faunaDB",
    "helper",
    "utility"
  ],
  "author": "Marc-Aurele Besner <82244926+marc-aurele-besner@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marc-aurele-besner/faunadb-utility/issues"
  },
  "homepage": "https://github.com/marc-aurele-besner/faunadb-utility#readme",
  "devDependencies": {
    "@types/node": "^18.13.0",
    "awesome-readme": "^0.0.8",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "faunadb": "^4.8.0"
  }
}
